        public static void contar(int num1, int num2) {
               if (num1 > num2) {
            throw new IllegalArgumentException("O primeiro parâmetro deve ser menor ou igual ao segundo parâmetro.");
        }
               for (int i = num1; i <= num2; i++) {
            System.out.print(i + " ");
        }
        System.out.println(); 
